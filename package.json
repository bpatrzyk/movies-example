{
  "name": "movies-example",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "lint-check": "tslint -p tsconfig.json -c tslint.json",
    "build": "npm run clean && tsc",
    "start": "node .",
    "start-dev": "npm run build && npm run start",
    "dev": "nodemon --watch src -e ts --exec npm run start-dev",
    "test": "jest",
    "coverage": "jest --coverage",
    "db:up": "knex migrate:up",
    "db:down": "knex migrate:down",
    "db:latest": "knex migrate:latest",
    "db:teardown": "knex migrate:rollback --all"
  },
  "license": "MIT",
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/.jest/setEnvVars.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,md}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-validator": "^3.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.9",
    "@types/node-fetch": "^2.5.3",
    "@types/pg": "^7.11.2",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/winston": "^2.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "express-winston": "^4.0.1",
    "knex": "^0.20.2",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.7",
    "pg": "^7.12.1",
    "rimraf": "^3.0.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
  }
}
